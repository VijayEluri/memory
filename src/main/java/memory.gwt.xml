<module>
  <inherits name="com.google.gwt.user.User"/>
  <inherits name="com.google.gwt.resources.Resources"/>
  <!--<inherits name="com.google.gwt.i18n.I18N"/>-->
  <inherits name="com.threerings.gwt.Utils"/>
  <inherits name="com.allen_sauer.gwt.dnd.gwt-dnd"/>

  <source path="memory/client"/>
  <source path="memory/data"/>
  <source path="memory/rpc"/>
  <public path="memory/css"/>
  <stylesheet src="standard.css"/>
  <entry-point class="memory.client.MemoryClient"/>

  <!-- we only support standards compliant browers; meh -->
  <set-property name='user.agent' value='safari'/>
  <set-configuration-property name="user.agent.runtimeWarning" value="false" />

  <!-- enable superdevmode -->
  <add-linker name="xsiframe"/>
  <set-configuration-property name="devModeRedirectEnabled" value="true"/>
</module>
